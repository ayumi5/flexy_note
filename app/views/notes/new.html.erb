<%= form_for @note do |f| %>
  <%= f.label :title %><br/>
  <%= f.text_field :title %><br/>
  <%= f.label :category %><br/>
  <%= f.text_field :category %><br/>
  <%= f.label :note %><br/>
  <%= f.text_area :content %><br/>
  <%= f.label :url %><br/>
  <%= f.text_field :url %><br/>
  <%= f.submit :create %>
<% end %>
